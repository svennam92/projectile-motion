<script src='https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
<script src='/vendor/lodash.min.js'></script>
<script src='/js/scripts.js'></script>

<script>
  var guess = {{{jsonStringify data.guess}}};
  var auth = {{{jsonStringify data.auth}}};
  var flows = {{{jsonStringify data.flows}}};
  var state = {{{jsonStringify data.state}}};
  {{#if data.score}}
    var score = {{{jsonStringify data.score}}};
  {{else}}
    var score = 0;
  {{/if}}

  var width = $('.canvas').attr('width');
  var height = $('.canvas').attr('height');

  var target_distance = state.target_distance;

  var canvas = $('.canvas')[0];
  var myGame = new ProjectileMotion.Game(canvas, auth, flows, width, height, guess, target_distance);
  myGame.start();

</script>
